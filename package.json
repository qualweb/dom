{
  "name": "@qualweb/dom",
  "version": "0.2.2",
  "description": "Processes necessary page data for qualweb accessibility evaluation.",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "mocha --require esm",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prebuild": "rimraf dist",
    "build": "npm run format && npm run lint && npm run prebuild && tsc --build",
    "prepare": "npm run build"
  },
  "homepage": "https://github.com/qualweb/dom#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualweb/dom.git"
  },
  "bugs": {
    "url": "https://github.com/qualweb/dom/issues"
  },
  "keywords": [
    "qualweb",
    "accessibility",
    "dom",
    "lib"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "Jo√£o Vicente",
  "license": "ISC",
  "devDependencies": {
    "@qualweb/types": "0.6.9",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^15.12.5",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chai": "^4.3.4",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "esm": "^3.2.25",
    "mocha": "^8.4.0",
    "prettier": "^2.3.2",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.21.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
